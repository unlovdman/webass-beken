{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/[...path].js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
} 